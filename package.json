{
	"name": "vite-plugin-control-add-in",
	"version": "0.0.2",
	"description": "Vite plugin to write Microsoft Control Add-in's.",
	"author": {
		"name": "Hugo Korte",
		"email": "hugokorteapple@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Hugos68/vite-plugin-control-add-in"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": ["vite", "vite-plugin", "microsoft", "control-add-in"],
	"scripts": {
		"build": "unbuild",
		"format": "biome check --write",
		"format:check": "biome check",
		"prepack": "pnpm build"
	},
	"files": ["dist"],
	"sideEffects": false,
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"default": "./dist/index.mjs",
				"types": "./dist/index.d.mts"
			},
			"require": {
				"default": "./dist/index.cjs",
				"types": "./dist/index.d.cts"
			}
		}
	},
	"peerDependencies": {
		"vite": ">=6.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.27.11",
		"@types/node": "^22.10.5",
		"unbuild": "^3.2.0",
		"vite": "^6.0.7",
		"vitest": "^2.1.8"
	},
	"license": "MIT",
	"type": "module"
}
